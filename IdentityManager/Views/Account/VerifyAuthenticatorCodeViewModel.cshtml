@model VerifyAuthenticatorViewModel

<h1> Verify Authenticator Code</h1>
<form asp-controller="Account" asp-action="VerifyAuthencatorCode" asp-route="@ViewData["ReturnUrl"]" method="post" role="form">

    <div asp-validation-summary="All" class="text-danger"></div>
    <input asp-for="RemenberMe" type="hidden" />
    <hr />
    <div class=" form-group">
        <label asp-for="Code" class="col-md-2"></label>
        <div class=" col-md-10">
            <input asp-for=" Code" class=" form-control" />
            <span asp-validation-for="Code" class="text-danger"></span>
        </div>

    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-success"></button>
    </div>

</form>

@section Scripts{
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

